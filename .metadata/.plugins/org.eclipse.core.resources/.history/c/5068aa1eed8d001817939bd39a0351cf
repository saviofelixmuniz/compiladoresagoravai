grammar org.xtext.example.go.Go with org.eclipse.xtext.common.Terminals

generate go "http://www.xtext.org/example/go/Go"

Model:
	greetings+=Greeting*;
	
Greeting:
	'Hello' AritExp* '!';

AritExp:
	Multiplication (('+'|'-') Multiplication)*
;

Multiplication:
	Primary (('*'|'/') Primary)*
;

Primary:
	NumberLiteral | '(' AritExp ')'
;

NumberLiteral:
	INT
;

terminal OP_REL:
	"==" | "!=" | "<=" | ">=" | "<" | ">"	
;

terminal OP_BOOL:
	"&&" | "||" | "!"
;

terminal BOOL:
	"true" | "false"
;

